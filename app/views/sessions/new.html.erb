<div id="wrapper">
<h1>Login</h1>
<%= form_tag sessions_path do %>
  
    <h2><%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email]%> </h2>
  
  <div class="field">
    <%= label_tag :password %><br />
    <%= text_field_tag :password, params[:password], :type => "password" %> 
  </div>
  <div class="actions"><%= submit_tag "Log In" %></div>
<% end %>
</div>